{
  "version": 2,
  "builds": [
    {
      "src": "app.py",
      "use": "@vercel/python"
    },
    {
      "src": "public/css/styles.css",
      "use": "@vercel/static"
    },
    {
      "src": "public/js/calculator.js",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/css/styles.css",
      "dest": "/public/css/styles.css"
    },
    {
      "src": "/js/calculator.js",
      "dest": "/public/js/calculator.js"
    },
    {
      "src": "/(.*)",
      "dest": "/app.py"
    }
  ]
}
